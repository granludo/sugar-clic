  <revision description="Conversión de Clic3 a JClic" date="1/27/06">
   <author name="Equip Clic" mail="clic@xtec.net" organization="Àrea TIC - Departament d'Educació" />
  </revision>
  <author name="Félix Manuel Redondo Templado" mail="templado@teleline.es" />
  <organization name="IES Complutense" mail="ies.complutense@terra.es" url="http://centros5.pntic.mec.es/ies.complutense/" city="Alcalá de Henares" state="Madrid" />
  <language>español</language>
  <description>
   <p>Actividades relacionadas con la conversión entre sistemas de numeración: de binario a decimal, de decimal a binario, de hexadecimal a binario, etc...</p>
  </description>
  <descriptors area="Tecnología" level="Secundaria (12-16), Bachillerato (16-18)" />
 </settings>
