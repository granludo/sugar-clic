  <activity class="@textGrid.WordSearch" name="bytes.sop" code="">
   <description />
   <messages>
    <cell type="initial">
     <style />
     <p>Encuentra 4 prefijos que son múltiplos de byte.</p>
    </cell>
    <cell type="final">
     <style />
     <p>Perfecto!</p>
    </cell>
   </messages>
   <settings margin="8" report="true" reportActions="false">
    <container bgColor="0xC0C0C0">
     <counters time="true" actions="true" score="true" />
    </container>
    <window bgColor="0xFFFFFF" border="true" />
   </settings>
   <textGrid rows="9" columns="12" cellWidth="20.0" cellHeight="20.0" border="true">
    <style>
     <color background="0xFFFFFF" />
    </style>
    <text>
     <row>LAKZGJN**</row>
     <row>LFNACUI**</row>
     <row>URYGMZV**</row>
     <row>RUHIMMA**</row>
     <row>NQFGPMO**</row>
     <row>DZLAGEM**</row>
     <row>MEPZMDK**</row>
     <row>*********</row>
     <row>*T*******</row>
     <row>*E*KILO**</row>
     <row>*R*******</row>
     <row>*A*******</row>
    </text>
   </textGrid>
   <clues>
    <clue id="0">GIGA</clue>
    <clue id="1">MEGA</clue>
    <clue id="2">KILO</clue>
    <clue id="3">TERA</clue>
   </clues>
   <cells rows="4" cols="1" cellWidth="140.0" cellHeight="100.0" border="false" id="secondary">
    <style>
     <color background="0xFFFFFF" />
    </style>
    <shaper class="@Rectangular" cols="1" rows="4" />
    <cell>
     <p>1 gigabyte = 1024 megabyte</p>
    </cell>
    <cell>
     <p>1 megabyte = 1024 kilobyte</p>
    </cell>
    <cell>
     <p>1 kilobyte = 1024 byte</p>
    </cell>
    <cell>
     <p>1 terabyte = 1024 gigabyte</p>
    </cell>
   </cells>
   <layout position="AB" />
  </activity>
